                    <button id="deleteButton" onclick="deleteData('{{class}}','{{roll}}', '{{year}}')">Delete
                      Student</button>
                    <script>
                      async function deleteData(classNo, roll, year) {
                        try {
                          console.log('clicked');
                          const response = await fetch(`http://localhost:4000/delete-student/${classNo}/${roll}/${year}`, {
                            method: 'DELETE',
                            headers: {
                              'Content-Type': 'application/json',
                              // Add any additional headers if needed
                            },
                          });

                          if (response.ok) {
                            // The request was successful
                            console.log('Student record deleted successfully');
                            // Redirect the user to another page using JavaScript
                            window.location.href = `/class${classNo}-year-submit?selectedYear=${year}&message=Student record deleted successfully`;
                          } else {
                            // The request was not successful, handle the error
                            const data = await response.json();
                            console.error(data.error);
                          }
                        } catch (error) {
                          console.error('Error:', error.message);
                        }
                      }
                    </script>

