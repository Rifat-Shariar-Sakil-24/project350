            <form action="/class3-year-submit" method="get" id="yearForm">
              <label for="yearDropdown">Select Year:</label>
              <select id="yearDropdown" name="selectedYear" onchange="submitForm()">
                <option value="2011">2011</option>
                <option value="2012">2012</option>
                <option value="2013">2013</option>
                <option value="2014">2014</option>
                <option value="2015">2015</option>
                <option value="2016">2016</option>
                <option value="2017">2017</option>
                <option value="2018">2018</option>
                <option value="2019">2019</option>
                <option value="2020">2020</option>
                <option value="2021">2021</option>
                <option value="2022">2022</option>
                <option value="2023">2023</option>
              </select>

              <script>
                // Function to get URL parameter value by name
                function getUrlParameter(name) {
                  const urlParams = new URLSearchParams(window.location.search);
                  return urlParams.get(name);
                }

                function submitForm() {
                  // Submit the form when the user selects an option
                  document.getElementById('yearForm').submit();
                }

                // Set the selected attribute based on the URL parameter
                const selectedYearFromURL = getUrlParameter('selectedYear');
                if (selectedYearFromURL) {
                  const yearDropdown = document.getElementById('yearDropdown');
                  const optionToSelect = yearDropdown.querySelector(`option[value="${selectedYearFromURL}"]`);
                  if (optionToSelect) {
                    optionToSelect.selected = true;
                  }
                }
              </script>
